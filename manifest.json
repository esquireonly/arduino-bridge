{
  "module": "index.js",
  "single": 0,
  "nochannels": 0,
  "extratab": 0,
  "pluginfiles": 0,
  "restartOnChannelsChange": 0,
  "innerId": 1,
  "comment": "Плагин для обмена данными с Arduino по Serial порту",

  "validator": {
    "main": {
      "port": {
        "notempty": true
      },
      "baudRate": {
        "min": 9600,
        "max": 115200
      },
      "pollingInterval": {
        "min": 100,
        "max": 10000
      }
    }
  },

  "default_channel": {
    "chan": "R0",
    "r": 1,
    "w": 0,
    "desc": "Переменная Arduino"
  },

  "channel_folder_popup": {
    "add": [
      { "id": "channel", "title": "Новый канал" }
    ],
    "copypaste": false,
    "delete": true
  },

  "channel_popup": {
    "delete": true
  }
}